{
    "compilerOptions": {
      "target": "ES2020",
      "module": "ESNext",
      "moduleResolution": "node",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "outDir": "./dist",
      "rootDir": "./src",
      "noImplicitAny": false
    },
    "include": ["src"],
    "exclude": ["node_modules"]
}
  